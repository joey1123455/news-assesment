                    }
                },
                "status": {
                    "type": "string"
                }
            }
        },
        "models.Article": {
            "type": "object",
            "required": [
                "article_id"
            ],
            "properties": {
                "article_id": {
                    "type": "string"
                },
                "category": {
                    "type": "array",
